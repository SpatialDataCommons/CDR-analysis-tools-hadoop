{
  "hive_commands": [
    "ADD JAR /hadoop/hive/lib/cdrmobilitylib.jar",
	"ADD JAR /hadoop/hive/lib/cdrmobilitylibge.jar",
	"ADD JAR /hadoop/hive/lib/cdrmobilitylibjica.jar",
	"ADD JAR /hadoop/hive/lib/jts-1.12.jar",
	"ADD JAR /hadoop/hive/lib/jtsio-1.12.jar",
	"ADD JAR /hadoop/hive/lib/javaml-0.1.6.jar",
	"ADD JAR /hadoop/hive/lib/weka.jar",
	"ADD JAR /hadoop/hive/lib/libsvm.jar",
	"ADD JAR /hadoop/hive/lib/ajt-2.5.jar",
	"ADD JAR /hadoop/hive/lib/Jama-1.0.2.jar",
	"create temporary function FindPolygon as 'com.apichon.mobility.hive.udf.FindPolygon'",
	"create temporary function FindPolygon2 as 'com.apichon.mobility.hive.udf.FindPolygon2'",
	"create temporary function CreateTrajectoriesCDR as 'com.apichon.mobility.hive.udf.ge.CreateTrajectoriesCDR'",
	"create temporary function TripSegmentationCDR as 'com.apichon.mobility.hive.udf.ge.TripSegmentationCDR'",
	"create temporary function CreateTrajectoriesJICAWithZone as 'com.apichon.mobility.hive.udf.jica.CreateTrajectoriesJICAWithZone'",
	"create temporary function TripOD as 'com.apichon.mobility.hive.udf.jica.TripOD'",
	"create temporary function TripODStay as 'com.apichon.mobility.hive.udf.jica.TripODStay'",
	"set hive.exec.dynamic.partition.mode=nonstrict",
	"set hive.exec.dynamic.partition=true",
	"set hive.exec.max.dynamic.partitions.pernode=4000",
	"set hive.exec.max.created.files=150000",
	"set hive.enforce.bucketing=true",
	"set mapred.reduce.slowstart.completed.maps=0.98",
	"set mapred.job.reuse.jvm.num.tasks=50",
	"set mapred.child.java.opts=-Xmx2048m -XX:-UseGCOverheadLimit",
	"set hive.map.aggr.hash.percentmemory = 0.25",
	"ADD jar /hadoop/hive/lib/commons-pool-1.5.4.jar",
	"ADD jar /hadoop/hive/lib/commons-dbcp-1.4.jar",
	"ADD jar /hadoop/hive/lib/commons-lang-2.6.jar",
	"ADD jar /hadoop/hive/lib/commons-logging-1.1.3.jar",
	"ADD jar /hadoop/hive/lib/commons-math-2.1.jar",
	"ADD jar /hadoop/hive/lib/gt-api-9.3.jar",
	"ADD jar /hadoop/hive/lib/gt-data-9.3.jar",
	"ADD jar /hadoop/hive/lib/gt-epsg-hsql-12.2.jar",
	"ADD jar /hadoop/hive/lib/gt-main-9.3.jar",
	"ADD jar /hadoop/hive/lib/gt-metadata-9.3.jar",
	"ADD jar /hadoop/hive/lib/gt-opengis-9.3.jar",
	"ADD jar /hadoop/hive/lib/gt-referencing-9.3.jar",
	"ADD jar /hadoop/hive/lib/jsr-275-1.0-beta-2.jar",
	"ADD jar /hadoop/hive/lib/jts-1.13.jar",
	"ADD jar /hadoop/hive/lib/vecmath-1.3.2.jar",
	"ADD jar /hadoop/hive/lib/postgresql-9.3-1102.jdbc4.jar",
	"ADD jar /hadoop/hive/lib/postgis-jdbc-2.1.0SVN.jar",

	"ADD jar /hadoop/hive/lib/cdrinterpolationlib.jar",

	"create temporary function f_turkcellarray   as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.TurkcellCdrArrayUDAF'",
	"create temporary function f_organizearray   as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.TurkcellOrganizeTripUDF'",
	"create temporary function f_reallocation    as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.TurkcellReallocationUDF'",
	"create temporary function f_routing         as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.TurkcellRoutingUDF'",
	"create temporary function f_routing2         as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.TurkcellRoutingUDF2'",
	"create temporary function f_hmesh           as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.HMeshUDF'",
	"create temporary function f_dumptrip        as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.DumpTripUDF'",
	"create temporary function f_hmesharray      as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.HMeshArrayUDF'",
	"create temporary function DumpTripUDF      as 'jp.ac.ut.csis.pflow.turkcell.hive.udf.DumpTripUDF'",
	"create temporary function sp_distance as 'com.apichon.mobility.hive.udf.sp_distance'",
	"set hive.support.sql11.reserved.keywords=false",
	"set hive.fetch.task.conversion=minimal",

    "add file {poi_location}",
    "add file {osm_location}",
    "add file {voronoi_location}",
    "use {db_name}"
  ]
}